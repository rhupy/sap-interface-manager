{
  "name": "sap-interface-manager",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main/main.js",
  "scripts": {
    "dev:main": "tsc --watch",
    "dev:renderer": "vite",
    "dev:electron": "wait-on http://localhost:5174 && electron dist/main/main.js",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:main\" \"npm run dev:renderer\" \"npm run dev:electron\"",
    "build:renderer": "vite build",
    "build:main": "tsc",
    "build": "npm run build:renderer && npm run build:main && electron-builder"
  },
  "build": {
    "appId": "com.example.sap-interface-manager",
    "files": [
      "dist/**/*",
      "public/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "dist"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.7",
    "node-rfc": "^3.3.1",
    "oracledb": "^6.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sql-formatter": "^15.4.11",
    "styled-components": "^5.3.11",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/node": "^22.13.5",
    "@types/oracledb": "^6.5.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.5.1",
    "vite": "^6.1.1",
    "wait-on": "^8.0.2"
  }
}
